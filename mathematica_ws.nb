     RowBox[{
      RowBox[{"y", "[", "1", "]"}], "\[Equal]", "1"}]}], "}"}], ",", " ", 
   RowBox[{"(*", " ", 
    RowBox[{"two", " ", "equations", " ", "specified", " ", "here"}], " ", 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{"y", "[", "x", "]"}], ",", "x"}], "]"}]], "Input"]
